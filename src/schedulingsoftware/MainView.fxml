<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="576.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingsoftware.MainViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Customers">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Label>
      <TableView fx:id="tableCustomer" editable="true" layoutX="14.0" layoutY="46.0" prefHeight="200.0" prefWidth="775.0">
        <columns>
          <TableColumn fx:id="columnName" prefWidth="135.0" text="Name" />
          <TableColumn fx:id="columnAddress" prefWidth="171.0" text="Address" />
            <TableColumn fx:id="columnAddress2" prefWidth="92.0" text="Address2" />
            <TableColumn fx:id="columnCity" prefWidth="88.0" text="City" />
            <TableColumn fx:id="columnZip" prefWidth="109.0" text="Zip Code" />
            <TableColumn fx:id="columnPhone" prefWidth="94.0" text="Phone" />
            <TableColumn fx:id="columnCountry" prefWidth="85.0" text="Country" />
        </columns>
      </TableView>
      <Button fx:id="btnAddCustomer" layoutX="14.0" layoutY="256.0" mnemonicParsing="false" onAction="#handleBtnAddCustomerAction" prefHeight="25.0" prefWidth="139.0" text="Add Customer" />
      <Button fx:id="btnDeleteCustomer" layoutX="156.0" layoutY="256.0" mnemonicParsing="false" onAction="#handleBtnDeleteUser" prefHeight="25.0" prefWidth="139.0" text="Delete Customer" />
      <Label alignment="BOTTOM_LEFT" layoutX="134.0" layoutY="21.0" prefHeight="19.0" prefWidth="267.0" text="(Double click row to edit, press Enter to save changes.)">
         <font>
            <Font name="System Italic" size="11.0" />
         </font>
      </Label>
      <TableView fx:id="tableAppointment" layoutX="14.0" layoutY="331.0" prefHeight="200.0" prefWidth="405.0">
        <columns>
          <TableColumn fx:id="columnAptCurrentTitle" prefWidth="102.0" text="Title" />
          <TableColumn fx:id="columnAptCurrentType" prefWidth="88.0" text="Type" />
            <TableColumn fx:id="columnAptCurrentStart" prefWidth="100.0" text="Start" />
            <TableColumn fx:id="columnAptCurrentEnd" prefWidth="114.0" text="End" />
        </columns>
      </TableView>
      <DatePicker editable="false" layoutX="582.0" layoutY="331.0" prefHeight="25.0" prefWidth="203.0" promptText="Select Date to Filter Appts." />
      <TableView fx:id="tableAppointmentDateFiltered" layoutX="424.0" layoutY="360.0" prefHeight="171.0" prefWidth="361.0">
        <columns>
          <TableColumn fx:id="columnAptDateTitle" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="columnAptDateType" prefWidth="92.0" text="Type" />
            <TableColumn fx:id="columnAptDateStart" prefWidth="91.0" text="Start" />
            <TableColumn fx:id="columnAptDateEnd" prefWidth="102.0" text="End" />
        </columns>
      </TableView>
      <RadioButton layoutX="424.0" layoutY="335.0" mnemonicParsing="false" text="Week of" />
      <RadioButton layoutX="499.0" layoutY="335.0" mnemonicParsing="false" text="Month of" />
      <Button fx:id="btnExit" layoutX="685.0" layoutY="537.0" mnemonicParsing="false" onAction="#HandleBtnExitAction" prefHeight="25.0" prefWidth="100.0" text="Exit" />
      <Button fx:id="btnAddAppointment" disable="true" layoutX="14.0" layoutY="537.0" mnemonicParsing="false" onAction="#handleBtnAddAppointmentAction" prefHeight="25.0" prefWidth="139.0" text="Add Appointment" />
      <Button fx:id="btnDeleteAppointment" layoutX="156.0" layoutY="537.0" mnemonicParsing="false" onAction="#handleBtnDeleteUser" prefHeight="25.0" prefWidth="139.0" text="Delete Appointment" />
      <Label alignment="BOTTOM_LEFT" layoutX="14.0" layoutY="282.0" prefHeight="49.0" prefWidth="775.0" text="(Double click row to edit, press Enter to save changes. Select customer in customer table to view related appointments.)" wrapText="true">
         <font>
            <Font name="System Italic" size="11.0" />
         </font>
         <graphic>
            <Label fx:id="lblAppointments" text="All Appointments">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
         </graphic>
      </Label>
   </children>
</AnchorPane>
